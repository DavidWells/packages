{
  "name": "git-er-done",
  "version": "0.1.11",
  "description": "Utility for dealing with modified, created, deleted files since a git commit",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "exit-if-diff": "./bin/exit-if-diff.js",
    "run-if-diff": "./bin/run-if-diff.js"
  },
  "exports": {
    ".": "./src/index.js",
    "./get-commit": "./src/git/commits/getCommit.js",
    "./get-all-commits": "./src/git/commits/getAllCommits.js",
    "./get-first-commit": "./src/git/commits/getFirstCommit.js",
    "./get-last-commit": "./src/git/commits/getLastCommit.js",
    "./get-details": "./src/git/getDetails.js",
    "./get-root": "./src/git/getGitRoot.js",
    "./get-files": "./src/git/getGitFiles.js",
    "./get-file-at-commit": "./src/git/getFileAtCommit.js",
    "./get-file-dates": "./src/git/dates/getFileDates.js",
    "./get-remotes": "./src/git/remotes/getRemotes.js",
    "./check-diff": "./src/git/diff/checkDiff.js"
  },
  "scripts": {
    "test": "uvu . \\.test\\.js$",
    "check": "tsc --noEmit",
    "types": "tsc",
    "prepublishOnly": "npm run types",
    "publish": "git push origin && git push origin --tags",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "bin",
    "dist",
    "src",
    "types",
    "README.md"
  ],
  "dependencies": {
    "debug": "^4.1.1",
    "json5": "^2.1.1",
    "jsonpointer": "^4.0.1",
    "lodash.includes": "^4.3.0",
    "lodash.isobject": "^3.0.2",
    "lodash.keys": "^4.2.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.memoize": "^4.1.2",
    "micromatch": "^4.0.2",
    "parse-diff": "^0.6.0",
    "rfc6902": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidWells/components/tree/master/packages/util-git-info"
  },
  "devDependencies": {
    "@davidwells/extract-deps": "^0.0.5",
    "@davidwells/git-split-diffs": "file:./git-split-diffs-repo",
    "@types/node": "^22.10.1",
    "configorama": "^0.6.9",
    "minimatch": "^10.0.1",
    "typescript": "^5.7.2",
    "uvu": "^0.5.6"
  }
}
